#!/bin/bash
# -*- ENCODING: UTF-8 -*-

# verificacion primarias
meu_ip_fun () {
MIP=$(ip addr | grep 'inet' | grep -v inet6 | grep -vE '127\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}' | grep -o -E '[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}' | head -1)
MIP2=$(wget -qO- ipv4.icanhazip.com)
[[ "$MIP" != "$MIP2" ]] && IP="$MIP2" || IP="$MIP"
}

nombrevalue="${message_from_first_name}"
nome-us=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
con-limit=$(cat < /etc/ADM-db/limit)
#SENTENCIA DE VERIFICACION DE LIMITE
[[ ${nome-us} <= ${con-limit} ]] && {
#INICIO DE SI ES MENOR
gerar_key
exit
} || { 
no_gerar_key
exit
}

[[ -e /etc/newadm-instalacao ]] && BASICINST="$(cat /etc/newadm-instalacao)" || BASICINST="cabecalho menu_credito ferramentas limitera menu_inst PPub.py usercodes ferramentascodes limiterb menu-txt payloads ssl user-txt ferramentas-txt limiter.sh paysnd.sh trans verifica confdropbear PDirect.py v-local.log idioma_geral PGet.py pwd.pwd ultrahost idioma_menuinst menu optimizador POpen.py shadowsocks.sh fai2ban overtcp PPriv.py user"
SCPT_DIR="/etc/SCRIPT"
[[ ! -e ${SCPT_DIR} ]] && mkdir ${SCPT_DIR}
INSTA_ARQUIVOS="ADMVPS.zip"
DIR="/etc/http-shell"
LIST="lista-arq"
[[ -e /etc/menu_ito ]] && nomkey="$(cat /etc/menu_ito)" || nomkey="$(curl -sSL "https://www.dropbox.com/s/z38hj1mz3q9krso/menu_credito")"
[[ -e /etc/menu_numito ]] && menumito="$(cat /etc/menu_numito)" || menumito="https://t.me/ChumoGH_bot"


gerar_key () {
#echo -e "$(cat < /etc/ADM-db/resell)" > ${SCPT_DIR}/menu_credito
meu_ip_fun
valuekey="$(date | md5sum | head -c10)"
valuekey+="$(echo $(($RANDOM*10))|head -c 5)"
oUP=$(wget -qO- ifconfig.me)
fun_list "$valuekey"
keyfinal=$(ofus "$oUP:8888/$valuekey/$LIST")
local bot_retorno="$LINE\n"
bot_retorno+=" Estimadp $message_from_first_name $message_from_last_name Exitosamente ‚úÖ\n ‚ö†Ô∏è ùî∏ùïåùïãùïÜùîªùîºùïäùïã‚Ñùùïå‚ÑÇ‚ÑÇùïÄùïÜ‚Ñï ùîº‚Ñï ùüöùüú ‚ÑçùïÜ‚Ñùùî∏ùïä ‚ö†Ô∏è\n"
bot_retorno+="$LINE\n"
#bot_retorno+="  ‚ö†Ô∏è ùöÉùô¥ùôΩùô≥ùöÅùô∞ùöÇ ùô∞ùô≤ùô≤ùô¥ùöÇùôæ ùô∏ùôªùô∏ùôºùô∏ùöÉùô∞ùô≥ùôæ ‚ö†Ô∏è \n   üö´ Sin TU RESELLER EN LA KEY üö´\n"
bot_retorno+=" üõ°Ô∏è ùöÅùô¥ùöÇùô¥ùôªùôªùô¥ùöÅ ùô∞ùöåùöùùöûùöäùöï üõ°Ô∏è : $message_from_last_name\n"
bot_retorno+="$LINE\n"
bot_retorno+=" üí• ${keyfinal} üí•\n"
bot_retorno+="$LINE\n"
bot_retorno+="  üõ°Ô∏è  ùô∏ùöóùöúùöùùöäùöïùöäùöçùöòùöõ  ùôæùöèùöíùöåùöíùöäùöï  üõ°Ô∏è \n"
bot_retorno+="$LINE\n"
bot_retorno+="apt update -y;apt upgrade -y;wget -q https://www.dropbox.com/s/0qj3edi5dibcrqd/instala.sh; chmod 777 instala.sh; ./instala.sh\n"
bot_retorno+="$LINE\n"
bot_retorno+=" üß¨ Soporte : Ubuntu 14.04 - 16.04 - 18.04 - 20.04\n         ùôçùôöùôòùô§ùô¢ùôöùô£ùôôùôñùôôùô§ ùôêùôóùô™ùô£ùô©ùô™ 18.04 \n  ü™¶ Solicita Creditos de tu KEY üõ°Ô∏è‚öîÔ∏è\n"
bot_retorno+=" üîê „Äé $numkey_used „ÄèùôÜùôöùôÆùô® ùôêùô®ùôñùôôùôñùô® üîì,„Äé $numkey_gen „ÄèùôÜùôöùôÆùô® ùôÇùôöùô£ùôöùôßùôñùôôùôñùô® üóùÔ∏è \n"
bot_retorno+=" ‚åö‚åö Hora Actual del Bot : $(printf '%(%D-%H:%M:%S)T')‚åö‚åö\n"
bot_retorno+="$LINE\n"
msj_fun
echo -e $bot_retorno >> ${keytxt}/key_${chatuser}.txt
upfile_fun ${keytxt}/key_${chatuser}.txt
rm ${keytxt}/key_${chatuser}.txt
#echo -e 'Âåö ÂçÑ „Ñ© Áà™ „Ññ ·é∂ ÂçÑ' > ${SCPT_DIR}/menu_credito
}

no_gerar_key () {
local bot_retorno="$LINE\n"
bot_retorno+="  ‚úò $message_from_first_name $message_from_last_name ‚úò Ya Usaste $numkey_used Keys  ‚úÖ\n ‚ö†Ô∏è Solicita Acceso Privado Ilimitado a $(cat < /etc/ADM-db/resell) ‚ö†Ô∏è\n"
bot_retorno+="$LINE\n"
bot_retorno+="  üõ°Ô∏è  Limite Aplicado  üõ°Ô∏è \n"
bot_retorno+="$LINE\n"
bot_retorno+=" üß¨ Soporte : Ubuntu 14.04 - 16.04 - 18.04 - 20.04\n         ùôçùôöùôòùô§ùô¢ùôöùô£ùôôùôñùôôùô§ ùôêùôóùô™ùô£ùô©ùô™ 18.04 \n  ü™¶ Solicita Creditos de tu KEY üõ°Ô∏è‚öîÔ∏è\n"
bot_retorno+=" üîê „Äé $numkey_used „ÄèùôÜùôöùôÆùô® ùôêùô®ùôñùôôùôñùô® üîì,„Äé $numkey_gen „ÄèùôÜùôöùôÆùô® ùôÇùôöùô£ùôöùôßùôñùôôùôñùô® üóùÔ∏è \n"
bot_retorno+=" ‚åö‚åö Hora Actual del Bot : $(printf '%(%D-%H:%M:%S)T')‚åö‚åö\n"
bot_retorno+="$LINE\n"
msj_fun
#echo -e $bot_retorno >> ${keytxt}/key_${chatuser}.txt
#upfile_fun ${keytxt}/key_${chatuser}.txt
#rm ${keytxt}/key_${chatuser}.txt
}


fun_list () {
rm ${SCPT_DIR}/*.x.c &> /dev/null
unset KEY
KEY="$1"
echo $KEY >> /root/ID-tses
#CRIA DIR
[[ ! -e ${DIR} ]] && mkdir ${DIR}
#ENVIA ARQS
i=0
VALUE+="gerar.sh instgerador.sh http-server.py lista-arq $BASICINST"
for arqx in `ls ${SCPT_DIR}`; do
[[ $(echo $VALUE|grep -w "${arqx}") ]] && continue 
echo -e "[$i] -> ${arqx}"
arq_list[$i]="${arqx}"
let i++
done
#CRIA KEY
[[ ! -e ${DIR}/${KEY} ]] && mkdir ${DIR}/${KEY}
#PASSA ARQS
#nombrevalue="${chatuser}"
nombrevalue="${message_from_first_name}"
echo "$nombrevalue" >> /etc/ADM-db/num-key.cont
numkey_used=$(grep -o -i $nombrevalue /etc/gerar-sh-log | wc -l)
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
#ADM BASIC
arqslist="$BASICINST"
for arqx in `echo "${arqslist}"`; do
[[ -e ${DIR}/${KEY}/$arqx ]] && continue #ANULA ARQUIVO CASO EXISTA
cp ${SCPT_DIR}/$arqx ${DIR}/${KEY}/
echo "$arqx" >> ${DIR}/${KEY}/${LIST}
done
rm ${SCPT_DIR}/*.x.c &> /dev/null
echo "$nombrevalue" > ${DIR}/${KEY}.name
[[ ! -z $IPFIX ]] && echo "$IPFIX" > ${DIR}/${KEY}/keyfixa
at now +24 hours <<< "rm -rf ${DIR}/${KEY} && rm -rf ${DIR}/${KEY}.name"
}

ofus () {
unset server
server=$(echo ${txt_ofuscatw}|cut -d':' -f1)
unset txtofus
number=$(expr length $1)
for((i=1; i<$number+1; i++)); do
txt[$i]=$(echo "$1" | cut -b $i)
case ${txt[$i]} in
".")txt[$i]="x";;
"x")txt[$i]=".";;
"1")txt[$i]="@";;
"@")txt[$i]="1";;
"2")txt[$i]="?";;
"?")txt[$i]="2";;
"4")txt[$i]="%";;
"%")txt[$i]="4";;
"/")txt[$i]="K";;
"K")txt[$i]="/";;
esac
txtofus+="${txt[$i]}"
done
echo "$txtofus" | rev
}
