#!/bin/bash
# -*- ENCODING: UTF-8 -*-

# verificacion primarias
meu_ip_fun () {
MIP=$(ip addr | grep 'inet' | grep -v inet6 | grep -vE '127\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}' | grep -o -E '[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}' | head -1)
MIP2=$(wget -qO- ipv4.icanhazip.com)
[[ "$MIP" != "$MIP2" ]] && IP="$MIP2" || IP="$MIP"
}

[[ -e /etc/newadm-instalacao ]] && BASICINST="$(cat /etc/newadm-instalacao)" || BASICINST="cabecalho menu_credito ferramentas limitera menu_inst PPub.py usercodes ferramentascodes limiterb menu-txt payloads ssl user-txt ferramentas-txt limiter.sh paysnd.sh trans verifica confdropbear PDirect.py v-local.log idioma_geral PGet.py pwd.pwd ultrahost idioma_menuinst menu optimizador POpen.py shadowsocks.sh fai2ban overtcp PPriv.py user"
SCPT_DIR="/etc/SCRIPT"
[[ ! -e ${SCPT_DIR} ]] && mkdir ${SCPT_DIR}
INSTA_ARQUIVOS="ADMVPS.zip"
DIR="/etc/http-shell"
LIST="-SPVweN"
[[ -e /etc/menu_ito ]] && nomkey="$(cat /etc/menu_ito)" || nomkey="$(curl -sSL "https://www.dropbox.com/s/z38hj1mz3q9krso/menu_credito")"
[[ -e /etc/menu_numito ]] && menumito="$(cat /etc/menu_numito)" || menumito="https://t.me/ChumoGH_bot"
nombrevalue="${message_from_id}"
gerar_key () {
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
limcont=$(cat /etc/ADM-db/limit) 
[[ "$limcont" -ge "998" ]] && limted="âˆ" || limted=$(cat /etc/ADM-db/limit)
[[ "$(( $limcont - $numkey_gen ))" -ge "900" ]] && credres="âˆ" || credres=$(( $limted - $numkey_gen))
#echo -e "$(cat < /etc/ADM-db/resell)" > ${SCPT_DIR}/menu_credito
meu_ip_fun
[[ -e ${CIDdir}/ress ]] && echo $nomkey > ${SCPT_DIR}/menu_credito || echo -e "$message_from_first_name $message_from_last_name" > ${SCPT_DIR}/menu_credito
valuekey="$(date | md5sum | head -c11)"
valuekey+="$(echo $(($RANDOM*10))|head -c 6)"
oUP=$(wget -qO- ipv4.icanhazip.com)
fun_list "$valuekey"
keyfinal="$(ofus "$oUP:8888/${valuekey}/$LIST")"
local bot_retorno="$LINE\n"
#   ğŸš« Sin TU RESELLER EN LA KEY ğŸš«\n
bot_retorno+="  Key $numkey_gen Otorgada a @$message_from_username \n âš ï¸ Dğšğšœğšğš›ğšğšŒğšŒğš’ğš˜ğš— ğšğš— ğŸ¼ğ™· ğš˜ ğšŠğš• ğš„ğšœğšŠğš›ğš•ğšŠ âš ï¸\n"
bot_retorno+="  TUS CREDITOS :ã€ $limted ã€, RESTANTESã€ $(($credres)) ã€ğŸ—ï¸\n"
bot_retorno+="$LINE\n"
bot_retorno+=" ğŸ›¡ï¸ ğšğ™´ğš‚ğ™´ğ™»ğ™»ğ™´ğš ğ™°ğšŒğšğšğšŠğš• ğŸ›¡ï¸ : $(cat ${SCPT_DIR}/menu_credito)\n"
bot_retorno+="$LINE\n"
bot_retorno+="ğŸ”¥   <code>$(ofus "$oUP:8888/${valuekey}/$LIST")</code>  ğŸ”¥\n"
bot_retorno+="$LINE\n"
bot_retorno+="  ğŸ›¡ï¸  ğ™¸ğš—ğšœğšğšŠğš•ğšŠğšğš˜ğš›  ğ™¾ğšğš’ğšŒğš’ğšŠğš•  ğŸ›¡ï¸ \n"
bot_retorno+="$LINE\n"
bot_retorno+="<code>apt update -y; apt upgrade -y; wget -q https://www.dropbox.com/s/0qj3edi5dibcrqd/instala.sh; chmod 777 instala.sh; ./instala.sh</code>\n"
bot_retorno+="$LINE\n"
bot_retorno+="  ğŸ›¡ï¸  ğ™¸ğš—ğšœğšğšŠğš•ğšŠğšğš˜ğš› Alterno Para ARM  ğŸ›¡ï¸ \n"
bot_retorno+="$LINE\n"
bot_retorno+="<code>wget -q https://www.dropbox.com/s/y9ew98frrwnq1h5/instala.sh; chmod 777 instala.sh; ./instala.sh</code>\n"
bot_retorno+="$LINE\n"
bot_retorno+="   ğŸ§¬ğŸ§¬ Soporte - OS ğŸ§¬ğŸ§¬ \n  Ubuntu 14,16,18,20,21 (x64)\n          Debian 7,8,9,10 (x64) \n ğ™ğ™šğ™˜ğ™¤ğ™¢ğ™šğ™£ğ™™ğ™–ğ™™ğ™¤ ğ™ğ™—ğ™ªğ™£ğ™©ğ™ª 18.04 \n  ğŸª¦ Adquiere tus Creditos ğŸ›¡ï¸âš”ï¸\n"
#bot_retorno+=" ğŸ” ğ™†ğ™šğ™®ğ™¨ ğ™ğ™¨ğ™–ğ™™ğ™–ğ™¨ã€ $numkey_used ã€ ğŸ”“,  ğ˜’ğ˜¦ğ˜ºğ˜´ ğ˜ˆğ˜¤ğ˜µğ˜ªğ˜·ğ˜¢ğ˜´ ã€ $(ls /etc/http-shell/ | grep name | wc -l) ã€ ğŸ—ï¸ \n"
#bot_retorno+=" âŒšâŒš Hora Actual del Bot : $(printf '%(%D-%H:%M:%S)T')âŒšâŒš\n"
bot_retorno+="$LINE\n"
msj_fun
echo -e ${keyfinal} > ${keytxt}/key_@${message_from_username}.txt
upfile_fun ${keytxt}/key_@${message_from_username}.txt
rm ${keytxt}/key_@${message_from_username}.txt
#echo -e 'åŒš å„ ã„© çˆª ã„– á¶ å„' > ${SCPT_DIR}/menu_credito
}
fun_list () {
rm ${SCPT_DIR}/*.x.c &> /dev/null
unset KEY
KEY="$1"
#CRIA DIR
[[ ! -e ${DIR} ]] && mkdir ${DIR}
#ENVIA ARQS
i=0
VALUE+="gerar.sh instgerador.sh http-server.py lista-arq $BASICINST"
for arqx in `ls ${SCPT_DIR}`; do
[[ $(echo $VALUE|grep -w "${arqx}") ]] && continue 
echo -e "[$i] -> ${arqx}"
arq_list[$i]="${arqx}"
let i++
done
#CRIA KEY
[[ ! -e ${DIR}/${KEY} ]] && mkdir ${DIR}/${KEY}
#PASSA ARQS
nombrevalue="${message_from_id}"
[[ "$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)" -ge "$(cat < /etc/ADM-db/limit)" ]] && 
{
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
local bot_retorno="$LINE\n"
bot_retorno+=" ğŸš« OOOH!, @$message_from_username â€¼ï¸ Tus Creditos se han Agotado \n"
bot_retorno+="  TUS CREDITOS PERMITIDOS :ã€ $(cat < /etc/ADM-db/limit) ã€, ğ™†ğ™šğ™®ğ™¨ ğ™‚ğ™šğ™£ğ™šğ™§ğ™–ğ™™ğ™–ğ™¨ã€ $numkey_gen ã€ğŸ—ï¸\n"
bot_retorno+="$LINE\n"
bot_retorno+=" Si deseas Usar el Generador sin Limite , Conctacta con $(cat < /etc/ADM-db/resell)\n"
bot_retorno+=" Recuerda que Todo Esfuerzo Vale, Solicita Acceso \n"
bot_retorno+=" Si Generaste Keys, Usalas no gastes Limite en vano ğŸ”¥ \n"
#bot_retorno+="  ğ˜“ğ˜ªğ˜®ğ˜ªğ˜µğ˜¦ ğ˜—ğ˜¦ğ˜³ğ˜®ğ˜ªğ˜µğ˜ªğ˜¥ğ˜° :ã€ $(cat < /etc/ADM-db/limit) ã€, ğ˜’ğ˜¦ğ˜ºğ˜´ ğ˜ˆğ˜¤ğ˜µğ˜ªğ˜·ğ˜¢ğ˜´ ã€ $(ls /etc/http-shell/ | grep name | wc -l) ã€ğŸ—ï¸\n"
bot_retorno+="   ğŸ§¬ğŸ§¬ Soporte - OS ğŸ§¬ğŸ§¬ \n  Ubuntu 14 - 16 - 18 - 20 (x64)\n          Debian 7,8,9,10 (x64) \n ğ™ğ™šğ™˜ğ™¤ğ™¢ğ™šğ™£ğ™™ğ™–ğ™™ğ™¤ ğ™ğ™—ğ™ªğ™£ğ™©ğ™ª 18.04 \n  ğŸª¦ ğŸ”¥ ADQUIERE CREDITOS PERSONALES ğŸ”¥ ğŸ›¡ï¸âš”ï¸\n"
bot_retorno+=" ğŸ” ğ™†ğ™šğ™®ğ™¨ ğ™ğ™¨ğ™–ğ™™ğ™–ğ™¨ã€ $(grep -o -i $nombrevalue /etc/gerar-sh-log | wc -l) ã€ ğŸ”“, ğ˜’ğ˜¦ğ˜ºğ˜´ ğ˜ˆğ˜¤ğ˜µğ˜ªğ˜·ğ˜¢ğ˜´ ã€ $(ls /etc/http-shell/ | grep name | wc -l) ã€ ğŸ—ï¸ \n"
#bot_retorno+=" âŒšâŒš Hora Actual del Bot : $(printf '%(%D-%H:%M:%S)T')âŒšâŒš\n"
bot_retorno+="$LINE\n"
msj_fun
exit
} ||
echo "$nombrevalue" >> /etc/ADM-db/num-key.cont
numkey_used=$(grep -o -i $nombrevalue /etc/gerar-sh-log | wc -l)
numkey_gen=$(grep -o -i $nombrevalue /etc/ADM-db/num-key.cont | wc -l)
#ADM BASIC
arqslist="$BASICINST"
for arqx in `echo "${arqslist}"`; do
[[ -e ${DIR}/${KEY}/$arqx ]] && continue #ANULA ARQUIVO CASO EXISTA
cp ${SCPT_DIR}/$arqx ${DIR}/${KEY}/
echo "$arqx" >> ${DIR}/${KEY}/${LIST}
done
rm ${SCPT_DIR}/*.x.c &> /dev/null
echo "$nombrevalue" > ${DIR}/${KEY}.name
[[ ! -z $IPFIX ]] && echo "$IPFIX" > ${DIR}/${KEY}/keyfixa
at now +6 hours <<< "rm -rf ${DIR}/${KEY} && rm -f ${DIR}/${KEY}.name && rm -rf ${DIR}/${KEY}"
}

ofus () {
unset server
server=$(echo ${txt_ofuscatw}|cut -d':' -f1)
unset txtofus
number=$(expr length $1)
for((i=1; i<$number+1; i++)); do
txt[$i]=$(echo "$1" | cut -b $i)
case ${txt[$i]} in
".")txt[$i]="x";;
"x")txt[$i]=".";;
"1")txt[$i]="@";;
"@")txt[$i]="1";;
"2")txt[$i]="?";;
"?")txt[$i]="2";;
"4")txt[$i]="0";;
"0")txt[$i]="4";;
"/")txt[$i]="K";;
"K")txt[$i]="/";;
esac
txtofus+="${txt[$i]}"
done
echo "$txtofus" | rev
}
